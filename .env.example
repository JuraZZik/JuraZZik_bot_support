# Enable automatic backups
BACKUP_ENABLED=false

# Create backup on bot startup
BACKUP_ON_START=false

# Backup entire project or just data files
BACKUP_FULL_PROJECT=false

# Which files to backup (if BACKUP_FULL_PROJECT=false)
BACKUP_FILE_LIST=data.json,banned.json

# Backup filename prefix
BACKUP_FILE_PREFIX=backup

# Interval between automatic backups (hours)
STORAGE_BACKUP_INTERVAL_HOURS=24

# How many days to keep backups
BACKUP_RETENTION_DAYS=7

# Patterns to exclude from backup (comma-separated)
BACKUP_EXCLUDE_PATTERNS=backups,bot.log,__pycache__,.git,.pyc,venv,*.log

# Send backup files to Telegram
BACKUP_SEND_TO_TELEGRAM=false

# Maximum backup file size to send to Telegram (MB)
BACKUP_MAX_SIZE_MB=100

